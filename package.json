  },
  "homepage": "https://github.com/hfg-gmuend/HfG-Emoji-Project-Website#readme",
  "devDependencies": {
    "budo": "^10.0.4",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "parallelshell": "^3.0.2"
  }
}
